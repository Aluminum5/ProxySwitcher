<UserControl x:Class="ProxySwitcher.Actions.DefaultActions.ChangePrinter.ChangePrinterConfig"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:res="clr-namespace:ProxySwitcher.Actions.DefaultActions"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <Grid>
        <Label Content="{x:Static res:DefaultResources.ChangePrinterTo_Label}" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="294" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,46,0,0" Name="comboBoxPrinter" VerticalAlignment="Top" Width="294" />
        <Button Content="{x:Static res:DefaultResources.Save_Button}" Height="23" HorizontalAlignment="Left" Margin="313,265,0,0" Name="buttonSave" VerticalAlignment="Top" Width="75" Click="buttonSave_Click" />
        <Button Content="{x:Static res:DefaultResources.Refresh_Button}" Height="23" HorizontalAlignment="Left" Margin="313,46,0,0" Name="buttonRefresh" VerticalAlignment="Top" Width="75" Click="buttonRefresh_Click" />
    </Grid>
</UserControl>
